{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-blog-cards-{{ ai_gen_id }} {
    padding: 40px 20px;
  }

  .ai-blog-cards__grid-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 30px;
    max-width: 1600px;
    margin: 0 auto;
  }

  .ai-blog-card-{{ ai_gen_id }} {
    background-color: {{ block.settings.card_background_color }};
    border-radius: {{ block.settings.card_border_radius }}px;
    overflow: hidden;
    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);
    transition: transform 0.3s ease, box-shadow 0.3s ease;
    display: flex;
    flex-direction: column;
  }

  .ai-blog-card-{{ ai_gen_id }}:hover {
    transform: translateY(-4px);
    box-shadow: 0 4px 16px rgba(0, 0, 0, 0.15);
  }

  .ai-blog-card__image-wrapper-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    padding-bottom: 66.67%;
    overflow: hidden;
    background-color: #f4f4f4;
  }

  .ai-blog-card__image-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-blog-card__image-placeholder-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: #f4f4f4;
  }

  .ai-blog-card__image-placeholder-{{ ai_gen_id }} svg {
    width: 60%;
    height: 60%;
    opacity: 0.3;
  }

  .ai-blog-card__content-{{ ai_gen_id }} {
    padding: 24px;
    flex-grow: 1;
    display: flex;
    flex-direction: column;
  }

  .ai-blog-card__heading-{{ ai_gen_id }} {
    margin: 0 0 12px;
    font-size: {{ block.settings.heading_size }}px;
    color: {{ block.settings.heading_color }};
    line-height: 1.3;
  }

  .ai-blog-card__text-{{ ai_gen_id }} {
    margin: 0 0 20px;
    font-size: {{ block.settings.text_size }}px;
    color: {{ block.settings.text_color }};
    line-height: 1.6;
    flex-grow: 1;
  }

  .ai-blog-card__button-{{ ai_gen_id }} {
    display: inline-block;
    padding: 12px 24px;
    background-color: {{ block.settings.button_background_color }};
    color: {{ block.settings.button_text_color }};
    text-decoration: none;
    border-radius: {{ block.settings.button_border_radius }}px;
    font-size: {{ block.settings.button_text_size }}px;
    text-align: center;
    transition: background-color 0.3s ease;
    align-self: flex-start;
  }

  .ai-blog-card__button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_hover_color }};
  }

  @media screen and (max-width: 989px) {
    .ai-blog-cards__grid-{{ ai_gen_id }} {
      grid-template-columns: repeat(2, 1fr);
      gap: 20px;
    }
  }

  @media screen and (max-width: 749px) {
    .ai-blog-cards__grid-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      gap: 20px;
    }

    .ai-blog-cards-{{ ai_gen_id }} {
      padding: 30px 15px;
    }
  }
{% endstyle %}

