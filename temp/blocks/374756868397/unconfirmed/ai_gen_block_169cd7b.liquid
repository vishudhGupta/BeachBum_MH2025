{% endstyle %}

<shop-the-look-{{ ai_gen_id }}
  class="ai-shop-look-{{ ai_gen_id }}"
  {{ block.shopify_attributes }}
>
  <div class="ai-shop-look-container-{{ ai_gen_id }}">
    {% if block.settings.heading != blank or block.settings.subtitle != blank %}
      <div class="ai-shop-look-header-{{ ai_gen_id }}">
        {% if block.settings.heading != blank %}
          <h2 class="ai-shop-look-title-{{ ai_gen_id }}">{{ block.settings.heading }}</h2>
        {% endif %}
        {% if block.settings.subtitle != blank %}
          <p class="ai-shop-look-subtitle-{{ ai_gen_id }}">{{ block.settings.subtitle }}</p>
        {% endif %}
      </div>
    {% endif %}

    <div class="ai-shop-look-content-{{ ai_gen_id }}">
      <div class="ai-shop-look-image-section-{{ ai_gen_id }}">
        <div class="ai-shop-look-image-wrapper-{{ ai_gen_id }}">
          {% if block.settings.model_image %}
            <img
              src="{{ block.settings.model_image | image_url: width: 800 }}"
              alt="{{ block.settings.model_image.alt | escape }}"
              class="ai-shop-look-image-{{ ai_gen_id }}"
              loading="lazy"
              width="{{ block.settings.model_image.width }}"
              height="{{ block.settings.model_image.height }}"
            >
          {% else %}
            <div class="ai-shop-look-image-placeholder-{{ ai_gen_id }}">
              {{ 'hero-apparel-1' | placeholder_svg_tag }}
              <div class="ai-shop-look-empty-state-{{ ai_gen_id }}">
                Add a model image to get started
              </div>
            </div>
          {% endif %}

          {% for i in (1..5) %}
            {% liquid
              assign hotspot_enabled_key = 'hotspot_' | append: i | append: '_enabled'
              assign hotspot_x_key = 'hotspot_' | append: i | append: '_x'
              assign hotspot_y_key = 'hotspot_' | append: i | append: '_y'
              assign product_key = 'hotspot_' | append: i | append: '_product'

              assign hotspot_enabled = block.settings[hotspot_enabled_key]
              assign hotspot_x = block.settings[hotspot_x_key]
              assign hotspot_y = block.settings[hotspot_y_key]
              assign product = block.settings[product_key]
            %}

            {% if hotspot_enabled %}
              <div
                class="ai-shop-look-hotspot-{{ ai_gen_id }}"
                style="
                  top: {{ hotspot_y }}%;
                  left: {{ hotspot_x }}%;
                "
                data-hotspot-id="{{ i }}"
              >
                <button
                  class="ai-shop-look-hotspot-button-{{ ai_gen_id }}"
                  aria-label="View product {{ i }}"
                  data-product-index="{{ i }}"
                >
                  <svg
                    class="ai-shop-look-hotspot-icon-{{ ai_gen_id }}"
                    xmlns="http://www.w3.org/2000/svg"
                    viewBox="0 0 24 24"
                    fill="none"
                    stroke="currentColor"
                    stroke-width="2"
                    stroke-linecap="round"
                    stroke-linejoin="round"
                  >
                    <line x1="12" y1="5" x2="12" y2="19"></line>
                    <line x1="5" y1="12" x2="19" y2="12"></line>
                  </svg>
                </button>
              </div>
            {% endif %}
          {% endfor %}
        </div>
      </div>

      <div class="ai-shop-look-product-section-{{ ai_gen_id }}">
        {% assign has_products = false %}
        {% for i in (1..5) %}
          {% liquid
            assign hotspot_enabled_key = 'hotspot_' | append: i | append: '_enabled'
            assign product_key = 'hotspot_' | append: i | append: '_product'
            assign hotspot_enabled = block.settings[hotspot_enabled_key]
            assign product = block.settings[product_key]
          %}
          
          {% if hotspot_enabled and product != blank %}
            {% assign has_products = true %}
            <div
              class="ai-shop-look-product-card-{{ ai_gen_id }}"
              data-product-id="{{ i }}"
            >
              <div class="ai-shop-look-product-image-{{ ai_gen_id }}">
                {% if product.featured_image %}
                  <img
                    src="{{ product.featured_image | image_url: width: 400 }}"
                    alt="{{ product.featured_image.alt | escape }}"
                    loading="lazy"
                    width="400"
                    height="400"
                  >
                {% else %}
                  <div class="ai-shop-look-product-image-placeholder-{{ ai_gen_id }}">
                    {{ 'product-apparel-1' | placeholder_svg_tag }}
                  </div>
                {% endif %}
              </div>

              <div class="ai-shop-look-product-content-{{ ai_gen_id }}">
                <h3 class="ai-shop-look-product-title-{{ ai_gen_id }}">{{ product.title }}</h3>
                
                <div class="ai-shop-look-product-price-{{ ai_gen_id }}">
                  {{ product.price | money }}
                  {% if product.compare_at_price > product.price %}
                    <span style="text-decoration: line-through; opacity: 0.6; margin-left: 8px;">
                      {{ product.compare_at_price | money }}
                    </span>
                  {% endif %}
                </div>

                {% if product.description != blank %}
                  <div class="ai-shop-look-product-description-{{ ai_gen_id }}">
                    {{ product.description | strip_html | truncate: 100 }}
                  </div>
                {% endif %}

                <a
                  href="{{ product.url }}"
                  class="ai-shop-look-product-button-{{ ai_gen_id }}"
                >
                  {{ block.settings.button_text }}
                </a>
              </div>
            </div>
          {% elsif hotspot_enabled %}
            <div
              class="ai-shop-look-product-card-{{ ai_gen_id }}"
              data-product-id="{{ i }}"
            >
              <div class="ai-shop-look-empty-product-{{ ai_gen_id }}">
                <h3>Product {{ i }}</h3>
                <p>Select a product for this hotspot</p>
              </div>
            </div>
          {% endif %}
        {% endfor %}

        {% unless has_products %}
          <div class="ai-shop-look-default-message-{{ ai_gen_id }}">
            <p>Enable hotspots and add products to showcase your look</p>
          </div>
        {% endunless %}
      </div>
    </div>
  </div>
</shop-the-look-{{ ai_gen_id }}>

<script>
  (function() {
    class ShopTheLook{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.hotspotButtons = [];
        this.productCards = [];
        this.currentActiveIndex = null;
      }

      connectedCallback() {
        this.hotspotButtons = this.querySelectorAll('.ai-shop-look-hotspot-button-{{ ai_gen_id }}');
        this.productCards = this.querySelectorAll('.ai-shop-look-product-card-{{ ai_gen_id }}');
        
        this.setupEventListeners();
        this.showFirstProduct();
      }

      setupEventListeners() {
        this.hotspotButtons.forEach((button) => {
          button.addEventListener('click', (e) => {
            const productIndex = button.getAttribute('data-product-index');
            this.showProduct(productIndex);
          });
        });
      }

      showProduct(index) {
        this.hotspotButtons.forEach((btn) => {
          btn.classList.remove('active');
        });

        this.productCards.forEach((card) => {
          card.classList.remove('active');
        });

        const activeButton = this.querySelector(`[data-product-index="${index}"]`);
        const activeCard = this.querySelector(`[data-product-id="${index}"]`);

        if (activeButton) {
          activeButton.classList.add('active');
        }

        if (activeCard) {
          activeCard.classList.add('active');
        }

        this.currentActiveIndex = index;
      }

      showFirstProduct() {
        if (this.hotspotButtons.length > 0) {
          const firstButton = this.hotspotButtons[0];
          const firstIndex = firstButton.getAttribute('data-product-index');
          this.showProduct(firstIndex);
        }
      }
    }

    customElements.define('shop-the-look-{{ ai_gen_id }}', ShopTheLook{{ ai_gen_id }});
  })();
</script>

