      height: auto;
      min-height: 100vh;
      max-height: none;
      overflow: visible;
    }

    .ai-shop-look-content-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      gap: 30px;
      height: auto;
    }

    .ai-shop-look-image-wrapper-{{ ai_gen_id }} {
      height: 70vh;
      min-height: 500px;
      max-height: 600px;
    }

    .ai-shop-look-product-section-{{ ai_gen_id }} {
      height: auto;
      min-height: 400px;
    }

    .ai-shop-look-product-card-{{ ai_gen_id }} {
      position: relative;
      height: auto;
      min-height: 400px;
    }

    .ai-shop-look-{{ ai_gen_id }} {
