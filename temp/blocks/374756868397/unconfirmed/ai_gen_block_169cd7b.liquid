  @media screen and (max-width: 749px) {
    .ai-shop-look-title-{{ ai_gen_id }} {
      font-size: calc({{ block.settings.heading_size }}px * 0.8);
    }

    .ai-shop-look-subtitle-{{ ai_gen_id }} {
      font-size: calc({{ block.settings.subtitle_size }}px * 0.9);
    }

    .ai-shop-look-image-wrapper-{{ ai_gen_id }} {
      height: 60vh;
      min-height: 400px;
      max-height: 500px;
    }

    .ai-shop-look-product-section-{{ ai_gen_id }} {
      padding: 20px 15px;
      min-height: 350px;
    }

    .ai-shop-look-product-card-{{ ai_gen_id }} {
      padding: 20px 15px;
      min-height: 350px;
    }

