    }

    .ai-shop-look-product-section-{{ ai_gen_id }} {
      padding: 25px 20px;
    }

    .ai-shop-look-product-card-{{ ai_gen_id }} {
      padding: 25px 20px;
    }

    .ai-shop-look-hotspot-button-{{ ai_gen_id }} {
      width: calc({{ block.settings.hotspot_size }}px * 0.8);
      height: calc({{ block.settings.hotspot_size }}px * 0.8);
    }

    .ai-shop-look-hotspot-icon-{{ ai_gen_id }} {
      width: calc({{ block.settings.hotspot_size }}px * 0.32);
      height: calc({{ block.settings.hotspot_size }}px * 0.32);
